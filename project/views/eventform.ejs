<!DOCTYPE html>
<html>
  <head>
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <link rel='stylesheet' href='/bootstrap/css/bootstrap.css' />
    <link rel='stylesheet' href='/bootstrap/css/bootstrap-responsive.css' />
    <script type = "text/javascript" src = "/javascripts/jquery-1.8.3.min.js"></script>
  </head>
  <body>
    <h1 id = "header"><a href = "/"><img src = "/images/flashmoblogo.png" id = "logo"/></a></h1>
    <form action = "/submitEvent" method = "get" id="loginbox">
      <div class="textbox" style="padding-top:10px;">
        <label for="name">Name:</label>
        <input type="text" name="name"/>
      </div>
      <div class="textbox" style="padding-top:10px;">
        <label for="address">Address:</label>
        <input type="text" name="address"/>
      </div>
      <div class="textbox" style="padding-top:10px;">
        <label for="city">City:</label>
        <input type="text" name="city"/>
      </div>
      <div class="textbox" style="padding-top:10px;">
        <label for="state">State:</label>
        <input type="text" name="state"/>
      </div>
      <div class="textbox" style="padding-top:10px;">
        <label for="zipcode">Zipcode:</label>
        <input type="text" name="zipcode"/>
      </div>
      <div class="textbox" style="padding-top:10px;">
        <label for="starttime">Start Time:</label>
        <input type="date" name="starttime">
      </div>
      <div class="textbox" style="padding-top:10px;">
        <label for="endtime">End Time:</label>
        <input type="date" name="endtime">
      </div>
      <p style="margin-left: -385px;">Description:</p> 
      <div class="textbox" style="margin-top:-30px;">
        <textarea name="description" rows = 12 cols=35></textarea>
      </div>
      <div>
        <input type = "submit" value = "Create" />
      </div>
    </form>
  </body>
</html>